<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
      <welcome-file>Student_register.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
      <servlet-class>com.jsp.servlet_simple_crud_operation.controller.StudentController</servlet-class>
      <servlet-name>Student_Register</servlet-name>
  </servlet>
  <servlet-mapping>
       <servlet-name>Student_Register</servlet-name>
       <url-pattern>/submit</url-pattern>
  </servlet-mapping>
  <servlet>
        <servlet-class>com.jsp.servlet_simple_crud_operation.controller.LoginStudentController</servlet-class>
        <servlet-name>login</servlet-name>
  </servlet>
  <servlet-mapping>
       <servlet-name>login</servlet-name>
       <url-pattern>/loginStudent</url-pattern>
  </servlet-mapping>
  <servlet>
     <servlet-class></servlet-class>
     <servlet-name>homepage</servlet-name>
  </servlet>
  <servlet-mapping>
      <servlet-name>homepage</servlet-name>
      <url-pattern></url-pattern>
  </servlet-mapping>
</web-app>
